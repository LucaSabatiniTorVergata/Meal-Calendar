<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #FFF8BF;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mealcalendar.view_controller.ResultsoDietViewController">

    <children>

        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d8b681" height="100.0" stroke="BLACK" strokeType="INSIDE" width="1280.0" />
        <Button fx:id="home" layoutX="14.0" layoutY="24.0" mnemonicParsing="false" onAction="#gohome" prefHeight="26.0" prefWidth="48.0" text="BACK" />
        <Label layoutX="118.0" layoutY="24.0" text="Resoconto fine dieta ...">
            <font><Font name="System Bold Italic" size="41.0" /></font>
        </Label>

        <ScrollPane layoutX="38.0" layoutY="126.0" prefHeight="566.0" prefWidth="750.0">
            <content>
                <VBox fx:id="resocontoBox" spacing="20.0">
                    <padding>
                        <Insets bottom="20" left="20" right="20" top="20" />
                    </padding>
                </VBox>
            </content>
        </ScrollPane>
      <Button fx:id="end" onAction="#endDiet" layoutX="1015.0" layoutY="200.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="102.0" text="FINE DIETA">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button onAction="#asknutr" fx:id="asknutrbutton" layoutX="969.0" layoutY="347.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="194.0" text="RESOCONTO NUTRIZIONISTA">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="987.0" layoutY="497.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="158.0" text="SEGUI NUTRIZIONISTA">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
